package bankImpl;
import bank.LoanAcc;
import bank.DebitInterest;

public class PersonalLoadAcc implements LoanAcc,DebitInterest
{
	public void repayPrincipal()
	{
		System.out.println("The repay principal of Personal Loan Acc");
	}
	public void payInterest()
	{
		System.out.println("The pay Interest of Personal Loan Acc");
	}
	public void payPartialPrincipal()
	{
		System.out.println("The pay partial principal of Personal Loan Acc");
	}
	public void deductMonthlyInt()
	{
		System.out.println("The deduct Monthly Interest of Personal Loan Acc");
	}
	public void deductHalfYrlyInt()
	{
		System.out.println("The amt to deduct half yearly Interest of Personal Loan Acc");
	}
	public void deductAnnualInt()
	{
		System.out.println("The amt to deduct Annual Interest of Personal Loan Acc");
	}
	public void calcInt()
	{
		System.out.println("To calculate interest of Personal Loan Acc");
	}
	public void creatAcc()
	{
		System.out.println("Function to create account of type Personal Loan Acc");
	}

	
}
